package Combine;

import java.util.Scanner;

public class FactorialusingRecursion {
	
	public static int recursion(int n) {
 for(int i=1;i<=n;i++) {
	 if(i==0||i==1)
		 return 1;
	 else 
		 return n*recursion(n-1);
	 
 }

 
	}

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		recursion(5);

	}

}
