package Combine;

import java.util.Scanner;

public class FactorialusingRecursion {
	
	public static int recursion(int n) {
		int fact=1;
 for(int i=1;i<=n;i++) {
	 if(i==0|| i==1)
		 return 1;
	 else {
		 return fact*recursion(n-1);
	 }
 }
 
	}

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();

	}

}
